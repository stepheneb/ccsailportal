<h1>Listing user_space_roles</h1>

<table>
  <tr>
    <th>User</th>
    <th>Space role</th>
  </tr>

<% for user_space_role in @user_space_roles %>
  <tr>
    <td><%=h user_space_role.user_id %></td>
    <td><%=h user_space_role.space_role_id %></td>
    <td><%= link_to 'Show', user_space_role %></td>
    <td><%= link_to 'Edit', edit_user_space_role_path(user_space_role) %></td>
    <td><%= link_to 'Destroy', user_space_role, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New user_space_role', new_user_space_role_path %>
