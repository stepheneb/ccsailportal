<h1>Editing offering</h1>

<% form_for(@offering) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :space_id %><br />
    <%= f.text_field :space_id %>
  </p>
  <p>
    <%= f.label :item_reference_id %><br />
    <%= f.text_field :item_reference_id %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @offering %> |
<%= link_to 'Back', offerings_path %>
